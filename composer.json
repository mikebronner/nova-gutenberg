{
    "name": "genealabs/nova-gutenberg",
    "description": "Implementation of the Gutenberg editor as a Laravel Nova Field based on Laraberg.",
    "keywords": [
        "laraberg",
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "2.*",
        "symfony/thanks": "^1.1",
        "unisharp/laravel-filemanager": "^1.9",
        "van-ons/laraberg": "0.0.6-beta"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaGutenberg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaGutenberg\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
